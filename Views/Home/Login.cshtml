@model UserLogin
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login</title>
</head>
<body>
    <a href="/">Register</a>
   <form asp-action="Login" asp-controller= "Home" method="POST">
    <div class="form-group">
        <span asp-validation-for="LogEmail"></span>
        <label asp-for="LogEmail">Email</label>
        <input type="LogEmail" asp-for="LogEmail">
    </div>

    <div class="form-group">
        <span asp-validation-for="LogPassword"></span>
        <label asp-for="LogPassword">Password</label>
        <input type="password" asp-for="LogPassword">
    </div>



    <input type="submit" value="Login">
</form> 
</body>
</html>